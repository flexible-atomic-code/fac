@SET_MAKE@

SHELL = /bin/sh

FC = @F77@
FFLAGS = @FFLAGS@
FFLAGSNO = @FFLAGSNO@

TOPDIR = @TOPDIR@

SRC = clogam.f f11.f coulcc.f y5n.f cphamp.f dlogam.f argam.f besljn.f beslik.f coulrad.f dcoul.f njsym.f njintr.f cmultip.f mohrfin.f

OBJS = ${SRC:.f=.o}

.f.o:
	${FC} -c ${FFLAGS} $<

coul: ${OBJS}
	$(FC) -c ${FFLAGSNO} d1mach.f
	ar r ${TOPDIR}/libfac.a ${OBJS} d1mach.o

clean:
	rm -f *.o *.a *~
